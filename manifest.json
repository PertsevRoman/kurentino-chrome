{
  "name": "WebRTC Chrome Extender",
  "author": "Pertsev Roman (income-software.org)",
  "version": "1.0",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "all_frames": true,
    "run_at": "document_start",
    "matches": ["*://genesis/*", "*://quins/*"]
  }],
  "manifest_version": 2,
  "description": "Расширение для включения возможностей передачи изображения рабочего стола и приложений посредством WebRTC",
  "externally_connectable": {
    "matches": ["*://genesis/*", "*://quins/*"]
  },
  "icons": {
    "48": "icon.png"
  },
  "permissions": [
    "desktopCapture",
    "*://genesis/*",
    "*://quins/*"
  ]
}
